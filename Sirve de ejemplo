            for (let i = 0 ; i < 8 ; i++) {
                if (i % 2 != 0) { 
            document.write("&nbsp");
            }
            for(let f = 0 ; f <= 8 ; f++){ 
                document.write("*")
            }
            document.write ("<br>")
        }
    </script>
   
